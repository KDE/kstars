      <entry name="RAMinimumPulse" type="UInt">
         <default>100</default>
      </entry>
      <entry name="DECMinimumPulse" type="UInt">
         <default>100</default>
      </entry>
      <entry name="GuidingRate" type="Double">
         <default>0.5</default>
      </entry>
      <entry name="GuiderAccuracyThreshold" type="UInt">
         <label>Accuracy threshold for the Guide Graphs.</label>
         <default>2</default>
      </entry>
      <entry name="RADisplayedOnGuideGraph" type="Bool">
         <label>Display the RA Plot on the Guide Drift Graphics.</label>
         <default>true</default>
      </entry>
      <entry name="DEDisplayedOnGuideGraph" type="Bool">
         <label>Display the DEC Plot on the Guide Drift Graphics.</label>
         <default>true</default>
      </entry>
      <entry name="RACorrDisplayedOnGuideGraph" type="Bool">
         <label>Display the RA Corrections Plot on the Guide Drift Graphics.</label>
         <default>false</default>
      </entry>
      <entry name="DECorrDisplayedOnGuideGraph" type="Bool">
         <label>Display the DEC Corrections Plot on the Guide Drift Graphics.</label>
         <default>false</default>
      </entry>
   </group>
   <group name="Scheduler">
    <entry name="SchedulerLogging" type="Bool">
      <whatsthis>Log Ekos Scheduler Module activity.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="SortSchedulerJobs" type="Bool">
      <whatsthis>Sort scheduler jobs by priority and altitude.</whatsthis>
      <default>true</default>
    </entry>
    <entry name="StopEkosAfterShutdown" type="Bool">
          <label>After shutdown procedure is successfully executed, shutdown INDI and Ekos.</label>
          <default>true</default>
    </entry>
    <entry name="ShutdownScriptTerminatesINDI" type="Bool">
          <label>Whether shutdown script, if exists, terminates INDI server in the process.</label>
          <default>false</default>
    </entry>
      <entry name="PreemptiveShutdown" type="Bool">
         <label>Perform pre-emptive shutdown if no jobs are due for a number of hours.</label>
         <default>false</default>
      </entry>
      <entry name="ResetMountModelOnAlignFail" type="Bool">
         <label>Reset mount model in case of alignment failure.</label>
         <default>false</default>
      </entry>
      <entry name="ResetMountModelBeforeJob" type="Bool">
         <label>Reset mount model before starting each job.</label>
         <default>false</default>
      </entry>
      <entry name="ResetGuideCalibration" type="Bool">
         <label>Always Reset guiding calibration before starting each job.</label>
         <default>true</default>
      </entry>
      <entry name="RealignAfterCalibrationFailure" type="Bool">
         <label>If guiding calibration fails, run alignment process again before proceeding to recalibration.</label>
         <default>false</default>
      </entry>
      <entry name="PreemptiveShutdownTime" type="Double">
         <label>Maximum number of hours before the next job is due to trigger a pre-emptive shutdown.</label>
         <default>2</default>
      </entry>
      <entry name="RememberJobProgress" type="Bool">
         <label>When processing a scheduled job, resume the sequence starting from the last image present in storage.</label>
         <default>true</default>
      </entry>
      <entry name="LeadTime" type="Double">
         <label>Minimum time between jobs in minutes.</label>
         <default>5</default>
      </entry>
      <entry name="PreDawnTime" type="Double">
         <label>Do not permit jobs to be scheduled or executed past this many minutes before dawn.</label>
         <default>30</default>
      </entry>
      <entry name="SettingAltitudeCutoff" type="Double">
         <label>Do not permit jobs to be scheduled less than this many degrees before the altitude restriction. Actual execution proceeds until the altitude limit.</label>
         <default>3</default>
      </entry>
      <entry name="DawnOffset" type="Double">
         <label>Offset astronomical dawn by this many hours to relax twilight restriction when using narrowband filters.</label>
         <default>0</default>
      </entry>
      <entry name="DuskOffset" type="Double">
         <label>Offset astronomical dusk by this many hours to relax twilight restriction when using narrowband filters.</label>
         <default>0</default>
      </entry>
      <entry name="TelescopeFocalLength" type="Double">
         <label>Telescope focal length in millimeters.</label>
         <default>0</default>
      </entry>
      <entry name="CameraPixelWidth" type="Double">
         <label>Camera pixel size width in micrometers.</label>
         <default>0</default>
      </entry>
      <entry name="CameraPixelHeight" type="Double">
         <label>Camera pixel size height in micrometers.</label>
         <default>0</default>
      </entry>
      <entry name="CameraWidth" type="UInt">
         <label>Camera Width in pixels.</label>
         <default>0</default>
      </entry>
      <entry name="CameraHeight" type="UInt">
         <label>Camera Height in pixels.</label>
         <default>0</default>
      </entry>
      <entry name="CameraRotation" type="Double">
         <label>Position angle of the camera with respect to north.</label>
         <default>0</default>
      </entry>
      <entry name="ErrorHandlingStrategy" type="UInt">
         <label>Strategy how to react, when a job aborts or steps into an error.</label>
         <default>1</default>
      </entry>
      <entry name="ErrorHandlingStrategyDelay" type="UInt">
         <label>Delay in minutes how long the scheduler should pause before restarting an aborted job or a job that ran into an error.</label>
         <default>0</default>
      </entry>
      <entry name="RescheduleErrors" type="Bool">
         <label>Re-schedule jobs that ran into errors.</label>
         <default>false</default>
      </entry>
   </group>
   <group name="INDI Lite">
      <entry name="LastServer" type="String">
         <label>The address of last used server</label>
      </entry>
      <entry name="LastServerPort" type="UInt">
         <label>The port of last used server</label>
         <default>7624</default>
      </entry>
      <entry name="LastWebManagerPort" type="UInt">
         <label>The port of last used Web Manager</label>
         <default>8624</default>
      </entry>
   </group>
