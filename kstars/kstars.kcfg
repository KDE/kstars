         <default code="true">KSUtils::getDefaultPath("AstrometryConfFile")</default>
      </entry>
      <entry name="AstrometryConfFileIsInternal" type="Bool">
         <label>Internal or External Astrometry.net Conf File?</label>
         <default>true</default>
      </entry>
      <entry name="UseDefaultPython" type="Bool">
         <label>Use the Default System Python Path</label>
         <default>true</default>
      </entry>
      <entry name="PythonExecPath" type="String">
         <label></label>
         <whatsthis>Folder in which the desired python executable or link to be used for astrometry.net resides.</whatsthis>
         <default>/usr/local/opt/python/libexec/bin</default>
      </entry>

      <entry name="UseSextractor" type="Bool">
