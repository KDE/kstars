      </entry>
      <entry name="AstrometryWCSIsInternal" type="Bool">
         <label>Internal or External wcsinfo?</label>
         <default>false</default>
      </entry>
      <entry name="AstrometryConfFile" type="String">
         <label>astrometry.net configuration file</label>
