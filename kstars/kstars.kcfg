         <default>false</default>
      </entry>
      <entry name="SextractorIsInternal" type="Bool">
         <label>Internal or External sextractor?</label>
         <default>false</default>
      </entry>
      <entry name="SextractorBinary" type="String">
         <label></label>
         <whatsthis>Path to the Sextractor executable.</whatsthis>
