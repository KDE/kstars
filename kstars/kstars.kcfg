      <entry name="ImagingPlannerShowNotPicked" type="Bool">
         <label>Show the objects that are not marked picked (don't show those that are picked).</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerDontCarePicked" type="Bool">
         <label>Object display doesn't depend on whether objects are marked picked or not.</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerShowKeyword" type="Bool">
         <label>Show the objects that have the keyword in their notes (and don't show those that don't).</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerShowNotKeyword" type="Bool">
         <label>Show the objects that don't have the keyword in their notes (and don't show those that do).</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerDontCareKeyword" type="Bool">
         <label>Object display doesn't depend on whether or not objects have the keyword in their notes.</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerKeyword" type="String">
         <label>Keyword used for the keyword filter.</label>
         <default></default>
      </entry>      
      <entry name="ImagingPlannerShowImaged" type="Bool">
         <label>Show the objects that are marked imaged (don't show those that are not marked imaged).</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerShowNotImaged" type="Bool">
         <label>Show the objects that are not marked imaged (don't show those that are marked imaged).</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerDontCareImaged" type="Bool">
         <label>Object display doesn't depend on whether objects are marked imaged or not.</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerShowAdded" type="Bool">
         <label>Show the objects that have been added (and don't show those that have not been added).</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerShowNotAdded" type="Bool">
         <label>Show the objects that have not been added (and don't show those that have been added.</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerDontCareAdded" type="Bool">
         <label>Object display doesn't depend on whether objects have been added or not.</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerShowIgnored" type="Bool">
         <label>Show the objects that have been ignored (and don't show those that have not been ignored).</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerShowNotIgnored" type="Bool">
         <label>Show the objects that have not been ignored (and don't show those that have been ignored).</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerDontCareIgnored" type="Bool">
         <label>Object display doesn't depend on whether objects have been ignored or not.</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerHideAstrobinDetails" type="Bool">
         <label>Hide the imaging planner's Astrobin search details section.</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerHideFilters" type="Bool">
         <label>Hide the imaging planner's filters section.</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerHideImage" type="Bool">
         <label>Hide the imaging planner's image section.</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerHideAltitudeGraph" type="Bool">
         <label>Hide the imaging planner's altitude graph.</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerCatalogPath" type="String">
         <label>File path to the most recent catalog loaded by the Imaging Planner.</label>
         <default></default>
      </entry>      
      <entry name="ImagingPlannerCenterOnSkyMap" type="Bool">
         <label>Slew to selected objects on the SkyMap.</label>
         <default>false</default>
      </entry>
      <entry name="ImagingPlannerIndependentWindow" type="Bool">
         <label>Make the Imaging Planner tool an independent window.</label>
         <default>true</default>
      </entry>
      <entry name="ImagingPlannerStartSolvingImmediately" type="Bool">
         <label>Start solving immediately after taking a screenshot.</label>
         <whatsthis>Start solving immediately after taking a screenshot, instead of waiting for the Solve button to be pressed.</whatsthis>
         <default>false</default>
      </entry>
