   <group name="Analyze">
    <entry name="AnalyzeRestartWithEkos" type="Bool">
       <label>If Analyze is running and Ekos is disconnected then reconnected, a new .analyze file will be created and written to, and the graphs in Analyze will only show the new session.</label>
       <default>true</default>
    </entry>
    <entry name="AnalyzeAlternativeDirectoryName" type="String">
       <label>Alternative directory to use for searching for analyze image files.</label>
       <default code="true">QDir::homePath()</default>
    </entry>
    <entry name="AnalyzeHFR" type="Bool">
      <whatsthis>Display HFR on the Analyze Statistics Plot.</whatsthis>
      <default>true</default>
    </entry>
    <entry name="AnalyzeNumCaptureStars" type="Bool">
      <whatsthis>Display number of stars detected in the capture on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeMedian" type="Bool">
      <whatsthis>Display median sample value for the capture on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeEccentricity" type="Bool">
      <whatsthis>Display the median eccentricity for the stars in the capture on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeTemperature" type="Bool">
      <whatsthis>Display the ambient temperature on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="FocusPosition" type="Bool">
      <whatsthis>Display the autofocus solution position.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeNumStars" type="Bool">
      <whatsthis>Display NumStars on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeSkyBg" type="Bool">
      <whatsthis>Display SkyBackground on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeSNR" type="Bool">
      <whatsthis>Display SNR on the Analyze Statistics Plot.</whatsthis>
      <default>true</default>
    </entry>
    <entry name="AnalyzeRA" type="Bool">
      <whatsthis>Display RA on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeDEC" type="Bool">
      <whatsthis>Display DEC on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeRAp" type="Bool">
      <whatsthis>Display RA Pulses on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeDECp" type="Bool">
      <whatsthis>Display DEC Pulses on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeDrift" type="Bool">
      <whatsthis>Display Drift on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeRMS" type="Bool">
      <whatsthis>Display RMS Error on the Analyze Statistics Plot.</whatsthis>
      <default>true</default>
    </entry>
    <entry name="AnalyzeTargetDistance" type="Bool">
      <whatsthis>Display the arc-seconds distance between the target position and the plate-solved captured image on the Analyze plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeRMSC" type="Bool">
      <whatsthis>Display RMS Error (during capture) on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeMountRA" type="Bool">
      <whatsthis>Display Mount RA on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeMountDEC" type="Bool">
      <whatsthis>Display Mount DEC on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeMountHA" type="Bool">
      <whatsthis>Display Mount Hour Angle on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeAz" type="Bool">
      <whatsthis>Display Azimuth on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeAlt" type="Bool">
      <whatsthis>Display Altitude on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzePierSide" type="Bool">
      <whatsthis>Display PierSide on the Analyze Statistics Plot.</whatsthis>
      <default>false</default>
    </entry>
    <entry name="AnalyzeStatsYAxis" type="String">
      <label>Stored Y-axis upper and lower limits for the Analyze Stats Plot.</label>
    </entry>
   </group>
   <group name="INDI Lite">
      <entry name="LastServer" type="String">
         <label>The address of last used server</label>
      </entry>
      <entry name="LastServerPort" type="UInt">
         <label>The port of last used server</label>
         <default>7624</default>
      </entry>
      <entry name="LastWebManagerPort" type="UInt">
         <label>The port of last used Web Manager</label>
         <default>8624</default>
      </entry>
   </group>
   <group name="HIPS">
    <entry name="HIPSMemoryCache" type="UInt">
          <label>RAM cache size in MB used to store cached HIPS images.</label>
          <default>300</default>
    </entry>
    <entry name="HIPSNetCache" type="UInt">
          <label>Hard disk cache size in MB used to store cached HIPS images.</label>
          <default>1000</default>
    </entry>
    <entry name="HIPSSource" type="String">
          <label>HIPS source catalog title.</label>
          <default>None</default>
    </entry>
    <entry name="HIPSBiLinearInterpolation" type="Bool">
          <label>Use Bilinear interpolation when rendering HiPS images?</label>
          <default>false</default>
    </entry>
    <entry name="HIPSShowGrid" type="Bool">
          <label>Show HiPS grid on the sky map.</label>
          <default>false</default>
    </entry>
    <entry name="HIPSPanning" type="Bool">
          <label>Redraw HiPS while panning.</label>
          <default>false</default>
    </entry>
    <entry name="ShowHIPS" type="Bool">
       <label>Draw HiPS sources in the sky map?</label>
       <whatsthis>Toggle whether the HIPS sources are drawn in the sky map.</whatsthis>
       <default>false</default>
    </entry>    
    <entry name="HIPSUseOfflineSource" type="Bool">
       <label>Use offline storage to load HiPS?</label>
       <default>false</default>
    </entry>
    <entry name="HIPSOfflinePath" type="String">
          <label>HIPS offline full path.</label>
    </entry>
   </group>
   <group name="Terrain">
    <entry name="TerrainSource" type="String">
          <label>Terrain Filename.</label>
          <whatsthis>Terrain source filename.</whatsthis>
          <default>None</default>
    </entry>
    <entry name="TerrainSourceCorrectAz" type="Int">
          <label>Terrain Azimuth Correction.</label>
          <whatsthis>Terrain source azimuth correction.</whatsthis>
          <default>0</default>
    </entry>
    <entry name="TerrainSourceCorrectAlt" type="Int">
          <label>Terrain Altitude Correction.</label>
          <whatsthis>Terrain source altitude correction.</whatsthis>
          <default>0</default>
    </entry>
    <entry name="TerrainDownsampling" type="Int">
          <label>Terrain Downsampling</label>
          <whatsthis>Speed quality tradeoff for rendering the terrain image.</whatsthis>
          <default>4</default>
    </entry>
    <entry name="TerrainPanning" type="Bool">
          <label>Terrain While panning.</label>
          <whatsthis>Redraw terrain while panning.</whatsthis>
          <default>true</default>
    </entry>
    <entry name="ShowTerrain" type="Bool">
       <label>Draw terrain</label>
       <whatsthis>Toggle whether the terrain is drawn in the sky map.</whatsthis>
       <default>false</default>
    </entry>
    <entry name="TerrainSkipSpeedup" type="Bool">
       <label>Terrain Skip Speedup</label>
       <whatsthis>Enable a one of the terrain drawing speedups.</whatsthis>
       <default>true</default>
    </entry>
    <entry name="TerrainTransparencySpeedup" type="Bool">
       <label>Terrain Transparency Speedup.</label>
       <whatsthis>Enable a one of the terrain drawing speedups.</whatsthis>
       <default>true</default>
    </entry>
    <entry name="TerrainSmoothPixels" type="Bool">
       <label>Terrain Smooth Pixels.</label>
       <whatsthis>Smooth pixels for a more pleasant, but slower rendering.</whatsthis>
       <default>false</default>
    </entry>
   </group>
   <group name="ImageOverlay">
    <entry name="ShowImageOverlays" type="Bool">
          <label>Display Image Overlays.</label>
          <whatsthis>Toggle whether to display image overlays.</whatsthis>
          <default>false</default>
    </entry>
    <entry name="ShowImageOverlaysBelowCatalogs" type="Bool">
          <label>Display Image Overlays below catalogs.</label>
          <whatsthis>Display Image Overlays below catalogs.</whatsthis>
          <default>false</default>
    </entry>
    <entry name="ShowSelectedImageOverlay" type="Bool">
          <label>Center SkyMap over selected image overlay.</label>
          <whatsthis>Center SkyMap over the selected overlay image in the image overlay table (if it's solved).</whatsthis>
          <default>true</default>
    </entry>
    <entry name="ImageOverlayMaxDimension" type="Int">
          <label>Image overlay max dimension</label>
          <whatsthis>Maximum dimension for image overlay images.</whatsthis>
          <default>1000</default>
    </entry>
    <entry name="ImageOverlayTimeout" type="Int">
          <label>Image overlay plate-solving timeout.</label>
          <whatsthis>Timeout for plate-solving an image overlay.</whatsthis>
          <default>30</default>
    </entry>
    <entry name="ImageOverlayDefaultScale" type="Double">
          <label>Image overlay default plate-solving scale.</label>
          <whatsthis>Default scale (arcseconds/pixel) for image-overlay plate solving.</whatsthis>
          <default>0</default>
    </entry>
   </group>
   <group name="Observatory">
   <entry name="DefaultObservatoryWeatherSource" type="String">
      <label>Default observatory module weather source.</label>
   </entry>
   <entry name="warningActionsActive" type="Bool">
      <label>Will be reacted upon warnings?</label>
      <default>true</default>
   </entry>
   <entry name="alertActionsActive" type="Bool">
      <label>Will be reacted upon alerts?</label>
      <default>true</default>
   </entry>
   <entry name="weatherWarningCloseDome" type="Bool">
      <label>Shall the dome being closed when a weather warning occurs?</label>
      <default>false</default>
   </entry>
   <entry name="weatherWarningCloseShutter" type="Bool">
      <label>Shall the shutter being closed when a weather warning occurs?</label>
      <default>false</default>
   </entry>
   <entry name="weatherWarningStopScheduler" type="Bool">
      <label>Shall the shutter being closed when a weather alert occurs?</label>
      <default>true</default>
   </entry>
   <entry name="weatherWarningDelay" type="UInt">
      <label>Delay for reacting upon a weather warning.</label>
      <default>600</default>
   </entry>
   <entry name="weatherAlertCloseDome" type="Bool">
      <label>Shall the dome being closed when a weather alert occurs?</label>
      <default>true</default>
   </entry>
   <entry name="weatherAlertCloseShutter" type="Bool">
      <label>Shall the shutter being closed when a weather alert occurs?</label>
      <default>true</default>
   </entry>
   <entry name="weatherAlertStopScheduler" type="Bool">
      <label>Shall the shutter being closed when a weather alert occurs?</label>
      <default>true</default>
   </entry>
   <entry name="weatherAlertDelay" type="UInt">
      <label>Delay for reacting upon a weather alert.</label>
      <default>30</default>
   </entry>
   <entry name="observatoryStatusUseDome" type="Bool">
      <label>Dome status relevant for the Observatory status.</label>
      <default>true</default>
   </entry>
   <entry name="observatoryStatusUseShutter" type="Bool">
      <label>Shutter status relevant for the Observatory status.</label>
      <default>true</default>
   </entry>
   <entry name="observatoryStatusUseWeather" type="Bool">
      <label>Weather status relevant for the Observatory status.</label>
      <default>true</default>
   </entry>
   <entry name="weatherAutoScaleValues" type="Bool">
      <label>Scale the sensor graph value axis to the values range.</label>
      <default>true</default>
   </entry>
   </group>
   <group name="ASTAP">
   <entry name="ASTAPExecutable" type="String">
      <label>Full path to the ASTAP executable.</label>
      <default code="true">KSUtils::getDefaultPath("ASTAP")</default>
   </entry>
   <entry name="ASTAPDownSample" type="Bool">
      <label>Down sample prior to solving. Also called binning. A value 0 will result in auto selection downsampling.</label>
      <default>false</default>
   </entry>
   <entry name="ASTAPDownSampleValue" type="Double">
      <label>Down sample prior to solving. Also called binning. A value 0 will result in auto selection downsampling.</label>
      <default>0</default>
   </entry>
   <entry name="ASTAPSearchRadius" type="Bool">
      <label>The program will search in a square spiral around the start position up to this radius.</label>
      <default>true</default>
   </entry>
   <entry name="ASTAPSearchRadiusValue" type="Double">
      <label>The program will search in a square spiral around the start position up to this radius.</label>
      <default>30</default>
   </entry>
   <entry name="ASTAPUpdateFITS" type="Bool">
      <label>Update the fits header with the found solution.</label>
      <default>false</default>
   </entry>
   <entry name="ASTAPLargeSearchWindow" type="Bool">
      <label>Increase search window size.</label>
      <default>false</default>
   </entry>
   </group>
   <group name="Mosaic">
   <entry name="MosaicTransparencyAuto" type="Bool">
     <whatsthis>Manage the mosaic panel transparency level automatically.</whatsthis>
     <default>true</default>
   </entry>
   <entry name="MosaicTransparencyLevel" type="Double">
     <whatsthis>Control mosaic panel transparency level.</whatsthis>
     <default>50</default>
   </entry>
   </group>
</kcfg>
