<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.kstars.Ekos">
    <property name="indiStatus" type="u" access="read"/>
    <property name="ekosStatus" type="u" access="read"/>
    <method name="connectDevices">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="disconnectDevices">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="getINDIConnectionStatus">
      <arg type="i" direction="out"/>
    </method>
    <method name="getEkosStartingStatus">
      <arg type="i" direction="out"/>
    </method>
    <method name="start">
      <arg type="b" direction="out"/>
    </method>
    <method name="stop">
      <arg type="b" direction="out"/>
    </method>
    <method name="getProfiles">
      <arg type="as" direction="out"/>
    </method>
    <method name="setProfile">
      <arg name="profileName" type="s" direction="in"/>
      <arg type="b" direction="out"/>
    </method>
    <signal name="indiStatusChanged">
        <arg name="status" type="u" direction="out"/>
    </signal>
    <signal name="ekosStatusChanged">
        <arg name="status" type="u" direction="out"/>
    </signal>
  </interface>
</node>
